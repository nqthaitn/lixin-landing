{
  "name": "tax-accounting-landing",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write 'src/**/*.{js,css}' '*.html'",
    "format:check": "prettier --check 'src/**/*.{js,css}' '*.html'"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@eslint/js": "^9.39.3",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "jsdom": "^28.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.97.0"
  },
  "lint-staged": {
    "src/**/*": {
      "js": [
        "eslint --fix",
        "prettier --write"
      ],
      "mjs": [
        "eslint --fix",
        "prettier --write"
      ]
    },
    "*": {
      "html": [
        "prettier --write"
      ],
      "css": [
        "prettier --write"
      ],
      "json": [
        "prettier --write"
      ]
    }
  }
}
